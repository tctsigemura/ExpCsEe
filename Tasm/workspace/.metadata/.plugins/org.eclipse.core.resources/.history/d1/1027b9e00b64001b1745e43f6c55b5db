package jp.ac.tokuyama.pico.tec6;

/**
 * @author sigemura
 * 文法エラー等の発生を知らせる例外
 */
class AsmException extends Exception {
  private Err err;

  AsmException(Err e) {
    err = e;
  }

  Err getErr() {
    return err;
  }
}
